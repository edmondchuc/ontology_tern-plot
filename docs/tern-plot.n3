plot:Site a rdfs:Class ;
    rdfs:label "Site" ;
    rdfs:comment """Location where observations may be made
May be a plot, quadrat, transect, trap etc

This is a sub-class of sosa:Sample because a site should be designed to to be representative of an environmental system (which may be an ecosystem or bioregion) or zone (which might be a fiat zone such as a parcel or tract)""",
        "ownership is associated with the parent survey",
        "rights and conditions are associated with the parnet survey" ;
    rdfs:subClassOf [ a owl:Restriction ;
            rdfs:comment "A site may be part of a bigger site -e.g. points within a plot" ;
            owl:allValuesFrom plot:Site ;
            owl:onProperty dcterms:hasPart ],
        [ a owl:Restriction ;
            rdfs:comment "A site may be part of a bigger site -e.g. points within a plot" ;
            owl:allValuesFrom plot:Site ;
            owl:onProperty dcterms:isPartOf ],
        [ a owl:Restriction ;
            owl:allValuesFrom plot:SiteVisit ;
            owl:onProperty plot:siteVisit ],
        [ a owl:Restriction ;
            owl:allValuesFrom geosparql:Feature ;
            owl:onProperty sosa:hasUltimateFeatureOfInterest ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty plot:mapsheetName ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty plot:mapsheetNumber ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty dcterms:identifier ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:comment ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:label ],
        [ a owl:Restriction ;
            owl:allValuesFrom skos:Concept ;
            owl:onProperty plot:mapScale ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://www.w3.org/ns/dcat#Dataset> ;
            owl:onProperty <http://www.w3.org/ns/dcat#dataset> ],
        [ a owl:Restriction ;
            owl:allValuesFrom sosa:Sample ;
            owl:onProperty sosa:hasSample ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/tern/ontologies/ssn/Method> ;
            owl:onProperty plot:locationMethod ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty dcterms:identifier ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty rdfs:label ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty <http://www.w3.org/ns/dcat#dataset> ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:mapScale ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:mapsheetName ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:mapsheetNumber ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:siteVisit ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty geosparql:hasGeometry ],
        [ a owl:Restriction ;
            owl:onProperty geosparql:hasGeometry ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( loc:Point loc:Polygon ) ] ],
        geosparql:SpatialObject,
        sosa:Sample .

plot-x:FluxTower a rdfs:Class,
        owl:Class ;
    rdfs:label "Flux tower" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty <http://www.w3.org/ns/locn#location> ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass sosa:Sensor ;
            owl:onProperty sosa:hosts ],
        sosa:Platform .

plot-x:TaxonAssignment a rdfs:Class,
        owl:Class ;
    rdfs:label "Taxon assignment observation" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue <http://www.tern.org.au/cv/op/Taxon-identity> ;
            owl:onProperty sosa:observedProperty ],
        sosa:Observation .

