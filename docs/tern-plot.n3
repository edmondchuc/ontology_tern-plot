plot:SamplingPoint a rdfs:Class,
        owl:Class ;
    rdfs:label "Sampling point" ;
    rdfs:comment "A sampling point within a site or along a transect." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom plot:Transect ;
            owl:onProperty plot:isSamplingPointOf ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty dcterms:identifier ],
        [ a owl:Restriction ;
            owl:allValuesFrom loc:Point ;
            owl:onProperty geosparql:hasGeometry ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:isSamplingPointOf ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty dcterms:identifier ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty geosparql:hasGeometry ],
        sosa:Sample,
        loc:Point .

plot:Site a rdfs:Class ;
    rdfs:label "Site" ;
    rdfs:comment """Location where observations may be made
May be a plot, quadrat, transect, trap etc

This is a sub-class of sosa:Sample because a site should be designed to to be representative of an environmental system (which may be an ecosystem or bioregion) or zone (which might be a fiat zone such as a parcel or tract)""",
        "ownership is associated with the parent survey",
        "rights and conditions are associated with the parnet survey" ;
    rdfs:subClassOf [ a owl:Restriction ;
            rdfs:comment "A site may be part of a bigger site -e.g. points within a plot" ;
            owl:allValuesFrom plot:Site ;
            owl:onProperty dcterms:hasPart ],
        [ a owl:Restriction ;
            rdfs:comment "A site may be part of a bigger site -e.g. points within a plot" ;
            owl:allValuesFrom plot:Site ;
            owl:onProperty dcterms:isPartOf ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://linked.data.gov.au/def/datatype/QuantitativeMeasure> ;
            owl:onProperty plot:area ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://linked.data.gov.au/def/datatype/QuantitativeMeasure> ;
            owl:onProperty plot:area ],
        [ a owl:Restriction ;
            owl:allValuesFrom plot:Dimension ;
            owl:onProperty plot:dimension ],
        [ a owl:Restriction ;
            owl:allValuesFrom plot:PropertyValue ;
            owl:onProperty schema:additionalProperty ],
        [ a owl:Restriction ;
            owl:allValuesFrom plot:SiteVisit ;
            owl:onProperty plot:siteVisit ],
        [ a owl:Restriction ;
            owl:allValuesFrom geosparql:Feature ;
            owl:onProperty sosa:hasUltimateFeatureOfInterest ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty prov:generatedAtTime ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty plot:locationDescription ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty plot:siteDescription ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty dcterms:identifier ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:comment ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:label ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://www.w3.org/ns/dcat#Dataset> ;
            owl:onProperty <http://www.w3.org/ns/dcat#dataset> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/tern/ontologies/ssn/CollectedSample> ;
            owl:onProperty sosa:hasSample ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/tern/ontologies/ssn/Method> ;
            owl:onProperty plot:locationMethod ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty dcterms:identifier ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty rdfs:label ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty <http://www.w3.org/ns/dcat#dataset> ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty prov:generatedAtTime ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:area ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:area ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:dimension ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:locationDescription ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:siteDescription ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty geosparql:hasGeometry ],
        [ a owl:Restriction ;
            owl:onProperty geosparql:hasGeometry ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( loc:Point loc:Polygon ) ] ],
        geosparql:SpatialObject,
        sosa:Platform,
        sosa:Sample .

plot:Transect a rdfs:Class,
        owl:Class ;
    rdfs:label "Transect" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom plot:SamplingPoint ;
            owl:onProperty plot:hasSamplingPoint ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty plot:transectDirection ],
        [ a owl:Restriction ;
            owl:allValuesFrom loc:LineString ;
            owl:onProperty geosparql:hasGeometry ],
        [ a owl:Restriction ;
            owl:allValuesFrom loc:Point ;
            owl:onProperty plot:transectEnd ],
        [ a owl:Restriction ;
            owl:allValuesFrom loc:Point ;
            owl:onProperty plot:transectStart ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:transectDirection ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:transectEnd ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:transectStart ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty geosparql:hasGeometry ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty plot:hasSamplingPoint ],
        sosa:Sample .

plot-x:FluxTower a rdfs:Class,
        owl:Class ;
    rdfs:label "Flux tower" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty <http://www.w3.org/ns/locn#location> ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass sosa:Sensor ;
            owl:onProperty sosa:hosts ],
        sosa:Platform .

plot-x:TaxonAssignment a rdfs:Class,
        owl:Class ;
    rdfs:label "Taxon assignment observation" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue <http://www.tern.org.au/cv/op/Taxon-identity> ;
            owl:onProperty sosa:observedProperty ],
        sosa:Observation .

