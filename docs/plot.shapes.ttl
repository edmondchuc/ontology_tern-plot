      sh:maxCount 1 ;
      sh:name "ended at time" ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://www.w3.org/ns/prov#startedAtTime> ;
      sh:datatype xsd:dateTime ;
      sh:maxCount 1 ;
      sh:name "started at time" ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
