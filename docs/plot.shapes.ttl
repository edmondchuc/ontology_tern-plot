:Site
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Site" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:property [
      a sh:PropertyShape ;
      sh:path plot:area ;
      sh:class <http://linked.data.gov.au/def/datatype/QuantitativeMeasure> ;
      sh:maxCount 1 ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path plot:dimension ;
      sh:class plot:Dimension ;
      sh:maxCount 1 ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path plot:locationDescription ;
      sh:datatype xsd:string ;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path plot:locationMethod ;
      sh:class <https://w3id.org/tern/ontologies/ssn/Method> ;
      sh:maxCount 1 ;
      sh:name "location method" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path plot:siteDescription ;
      sh:datatype xsd:string ;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path plot:siteVisit ;
      sh:class plot:SiteVisit ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://purl.org/dc/terms/hasPart> ;
      sh:class plot:SiteVisit ;
      sh:name "has part" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://purl.org/dc/terms/identifier> ;
      sh:datatype xsd:string ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:name "identifier" ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://purl.org/dc/terms/isPartOf> ;
      sh:class plot:SiteVisit ;
      sh:name "is part of" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://schema.org/additionalProperty> ;
      sh:class plot:PropertyValue ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://www.opengis.net/ont/geosparql#hasGeometry> ;
      sh:minCount 1 ;
      sh:name "has geometry" ;
      sh:nodeKind sh:BlankNode ;
      sh:or (
          [
            sh:class <https://w3id.org/tern/ontologies/loc/Point> ;
          ]
          [
            sh:class <https://w3id.org/tern/ontologies/loc/Polygon> ;
          ]
        ) ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path rdfs:comment ;
      sh:datatype xsd:string ;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path rdfs:label ;
      sh:datatype xsd:string ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://www.w3.org/ns/dcat#dataset> ;
      sh:class <http://www.w3.org/ns/dcat#Dataset> ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:name "dataset" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://www.w3.org/ns/prov#generatedAtTime> ;
      sh:datatype xsd:dateTime ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:name "generated at time" ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path sosa:hasSample ;
      sh:class <https://w3id.org/tern/ontologies/ssn/CollectedSample> ;
      sh:name "has sample" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path sosa:hasUltimateFeatureOfInterest ;
      sh:class <http://www.opengis.net/ont/geosparql#Feature> ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:targetClass plot:Site ;
.
:SiteVisit
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Site visit" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:property [
      a sh:PropertyShape ;
      sh:path plot:hadSubActivity ;
      sh:name "had sub activity" ;
      sh:nodeKind sh:IRI ;
      sh:or (
          [
            sh:class sosa:Observation ;
          ]
          [
            sh:class sosa:Sampling ;
          ]
        ) ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path plot:site ;
      sh:class plot:Site ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://purl.org/dc/terms/identifier> ;
      sh:datatype xsd:string ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:name "identifier" ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
