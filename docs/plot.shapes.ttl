      sh:name "was attributed to" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path sosa:isResultOf ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:name "is result of" ;
