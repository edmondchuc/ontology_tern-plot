plot:Dimension
  a owl:Class ;
  rdfs:comment "The dimension of a 2D feature." ;
  rdfs:label "Dimension" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://qudt.org/schema/qudt/Unit> ;
      owl:onProperty <http://linked.data.gov.au/def/datatype/unit> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty <http://linked.data.gov.au/def/datatype/unit> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty plot:length ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty plot:width ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty plot:length ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf (
              xsd:decimal
              xsd:integer
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty plot:width ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf (
              xsd:decimal
              xsd:integer
            ) ;
        ] ;
    ] ;
.
plot:Location
  a owl:Class ;
  rdfs:comment "Location together with information about how it was obtained" ;
  rdfs:label "Location" ;
